body /*主页背景统一样式*/
{
	margin: 0px;
	font: 12px/1.5 Arial,Tahoma,Helvetica,SimSun,sans-serif,Hei;
	color: #6f6f6f;
	height: 100%;
}
*
{
	margin: 0px;
	padding: 0px;
}
input, textarea, select, button
{
	color: #444;
	font: 12px/1.6em Verdana,Helvetica,Arial,sans-serif;
}
input, select, button
{
	vertical-align: middle;
	color: #444;
}
form
{
	padding: 0px;
	margin: 0px;
}
div
{
	overflow: hidden;
}
a
{
	text-decoration: none;
	color: #6f6f6f;
}
a:hover
{
	text-decoration: none;
}
a img
{
	border: none;
}
img
{
	border: none;
}
table
{
	border-collapse: collapse;
}
iframe
{
	width: 100%;
}
.unread_title
{
	font-weight: bold;
}
.unread_title a
{
	font-weight: normal;
}
a.red
{
	color: Red;
}
.green
{
	color: #42B475;
}
.mar_top_20
{
	margin-top: 20px;
}
.xuan
{
	width: 40px;
}
.blue
{
	color: #3399CC;
}
.green
{
	color: #009900;
}
.desk_icon
{
	background: url(  '../images/netdisk.jpg' ) left center no-repeat;
}
.folder_top
{
	width: 100%;

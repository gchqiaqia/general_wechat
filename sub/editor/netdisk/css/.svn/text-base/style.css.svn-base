.folder_title
{
	width: 100%;
	margin: 0px;
	border: 1px solid #C0BFBF;
	border-left: none;
	border-right: none;
	border-top: none;
	background-color: #F9F8F8;
}
.folder_title td
{
	height: 20px;
	padding-left: 10px;
	border-right: 1px solid #C0BFBF;
}

.folder_body
{
	width: 100%;
	margin: 0px;
	border: 1px solid #EBEBEB;
	border-left: none;
	border-top: none;
	background-color: #F9F8F8;
}
.folder_body tr
{
	height: 47px;
	padding-left: 10px;
	cursor: pointer;
}
.folder_body tr.on
{
	background-color: #9BC3EE;
}
.folder_body tr.on td
{
	color: White;
}
.folder_body tr.off
{
}
.folder_body tr.off:hover
{
	background-color: #F2F2F2;
}
.folder_body td
{
	height: 47px;
	padding-left: 10px;
	border-bottom: 1px solid #EBEBEB;
}
.folder_body td div
{
	display: none;
}
.folder_body td .icon
{
	height: 32px;
	width: 32px;
	line-height:32px;
	position:relative;
	margin:0px auto;
   text-align:center; 
   display: block;
   cursor: pointer;
}
.folder_body td .icon .img
{
	display:block;
	position:static;   
	height: 32px;
	width: 32px;
	background-image: url( '../images/icon-type_2.png' );
	background-repeat: no-repeat;
}
.folder_body td div.icon .share
{
	display: block;
	position: absolute;
	right: 0px;
	top: 0px;
	width: 32px;
	height: 32px;
	background-image: url( '../images/share_hand.png' );
	background-repeat: no-repeat;
	background-position: left bottom;
}
.folder_body td a:hover
{
	text-decoration: underline;
	color: Black;
}
.folder_body td .folder
{
	background-position: -242px 0px;
}
.folder_body td .mov
{
	background-position: -171px 0px;
}
.folder_body td .excel
{
	background-position: -136px 0px;
}
.folder_body td .ppt
{
	background-position: -206px 0px;
}
.folder_body td .word
{
	background-position: -104px 0px;
}
.folder_body td .image
{
	background-position: -70px 0px;
}
.folder_body td .pdf
{
	background-position: 0px -35px;
}
.folder_body td .txt
{
	background-position: -34px -35px;
}
.folder_body td .psd
{
	background-position: -70px -35px;
}
.folder_body td .rar
{
	background-position: -104px -35px;
}
.folder_body td .html
{
	background-position: -136px -35px;
}
.folder_body td .exe
{
	background-position: -171px -35px;
}
.folder_body td .other
{
	background-position: -206px -35px;
}


